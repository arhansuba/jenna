# Test Configuration
NODE_ENV=test

# Swarm Configuration
SWARM_MIN_MODELS=2
SWARM_MAX_MODELS=3
SWARM_VOTING_THRESHOLD=0.6
SWARM_DEFAULT_TIMEOUT=5000

# Database Configuration
SQLITE_FILE=:memory:
REDIS_URL=redis://localhost:6379

# API Keys (Use test keys or mock values)
OPENAI_API_KEY=test-openai-key
ANTHROPIC_API_KEY=test-anthropic-key
TWITTER_API_KEY=test-twitter-key
TWITTER_API_SECRET=test-twitter-secret
TWITTER_ACCESS_TOKEN=test-twitter-token
TWITTER_ACCESS_SECRET=test-twitter-secret

# Model Configuration
DEFAULT_MODEL=gpt-3.5-turbo
FALLBACK_MODEL=gpt-3.5-turbo
MODEL_TIMEOUT=5000

# Trading Configuration
TRADING_ENABLED=false
MAX_POSITION_SIZE=100
RISK_TOLERANCE=0.1
STOP_LOSS_PERCENTAGE=1
TAKE_PROFIT_PERCENTAGE=2

# Logging Configuration
LOG_LEVEL=error
LOG_FILE=logs/test.log