# API Keys
OPENAI_API_KEY=your_openai_api_key
ANTHROPIC_API_KEY=your_anthropic_api_key
GOOGLE_API_KEY=your_google_api_key
JUPITER_API_KEY=your_jupiter_api_key
HELIUS_API_KEY=your_helius_api_key
LLAMA_API_KEY=your_llama_api_key
TOGETHER_API_KEY=your_together_api_key

# Swarm Settings
SWARM_MIN_MODELS=2
SWARM_MAX_MODELS=5
SWARM_VOTING_THRESHOLD=0.6
SWARM_DEFAULT_TIMEOUT=30000

# Trading Settings
MAX_POSITION_SIZE=1000
STOP_LOSS_PERCENTAGE=2
TAKE_PROFIT_PERCENTAGE=5

# Database Settings
REDIS_HOST=localhost
REDIS_PORT=6379
REDIS_PASSWORD=your_redis_password

# Model Settings
GPT4_MAX_TOKENS=4000
GPT4_TEMPERATURE=0.7
CLAUDE2_MAX_TOKENS=2000
CLAUDE2_TEMPERATURE=0.5
LLAMA2_MAX_TOKENS=2000
LLAMA2_TEMPERATURE=0.6

# Social Media Settings
TWITTER_API_KEY=your_twitter_api_key
TWITTER_API_SECRET=your_twitter_api_secret
TWITTER_ACCESS_TOKEN=your_twitter_access_token
TWITTER_ACCESS_SECRET=your_twitter_access_secret

# Market Data Settings
MARKET_UPDATE_INTERVAL=300000  # 5 minutes in milliseconds
SENTIMENT_UPDATE_INTERVAL=600000  # 10 minutes in milliseconds
CONTENT_UPDATE_INTERVAL=900000  # 15 minutes in milliseconds

# Memory Settings
TRADING_MEMORY_RETENTION=30  # days
EMOTIONAL_MEMORY_RETENTION=7  # days

# Logging Settings
LOG_LEVEL=info
LOG_FILE_PATH=logs/app.log
ERROR_LOG_PATH=logs/error.log
